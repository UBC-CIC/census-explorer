{
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@styles": [
        "styles"
      ],
      "@data": [
        "data/*"
      ],
      "@types": [
        "types.ts"
      ],
      "@context/*": [
        "context/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@l10n/*": [
        "l10n/*"
      ]
    },
    "jsx": "preserve"
  }
}